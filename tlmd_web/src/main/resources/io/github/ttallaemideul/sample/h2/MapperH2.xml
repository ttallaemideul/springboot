<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.github.ttallaemideul.sample.h2.H2MapperH2">
	
	<select id="getServerNow" resultType="date">
	/* io.github.ttallaemideul.sample.h2.MapperH2.getServerNow */
		SELECT NOW()
	</select>
	
	<select id="getUsers" resultType="map">
	/* io.github.ttallaemideul.sample.h2.H2MapperH2.getUsers */
		SELECT
		FIRST_NAME, LAST_NAME, EMAIL, REG_DT
		FROM USERS
	</select>
</mapper>